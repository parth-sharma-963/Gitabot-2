{
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}